include "stdio.h"

# Conditionals

not : bool -> bool
not x = if x then false else true

xor : bool -> bool -> bool
xor a b = a && not b || not a && b

# Test laziness

putTrue : void -> bool
putTrue = do
    puts "true"
    true

putFalse : void -> bool
putFalse = do
    puts "false"
    false

testLazy : void -> void
testLazy = putTrue || putFalse
